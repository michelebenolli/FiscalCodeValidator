<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Library</OutputType>
    <TargetFramework>net5.0</TargetFramework>
    <RootNamespace>FiscalCodeValidator</RootNamespace>
    <ApplicationIcon />
    <StartupObject />
    <Authors>Michele Benolli</Authors>
    <Company>Graffiti</Company>
    <PackageTags>Fiscal Code, Validator</PackageTags>
    <Description>Fiscal Code Validator can be used to perform a syntactic validation of Italian fiscal codes.</Description>
    <PackageIcon>id-card.png</PackageIcon>
    <PackageIconUrl />
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <RepositoryUrl>https://github.com/michelebenolli/FiscalCodeValidator</RepositoryUrl>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <Copyright>Michele Benolli</Copyright>
    <PackageProjectUrl>https://github.com/michelebenolli/FiscalCodeValidator</PackageProjectUrl>
    <RepositoryType>git</RepositoryType>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
  </ItemGroup>

  <ItemGroup>
    <None Update="Data\places.json">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
    <None Update="Data\provinces.json">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
    <None Include="..\..\..\Users\michelebenolli\Downloads\id-card.png" Link="Data\id-card.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
